[user]
	name = brian
	email = brian@determined.ai
[push]
	default = current
[alias]
	b = branch
	ci = commit
	ca = commit --amend
	can = commit --amend --no-edit
	cmp = "!f() { git checkout master && git pull; }; f"
	co = checkout
	d = diff
	dh = diff HEAD
	fa = fetch --all
	nuke = "!f() { git push origin --delete $1; git branch -D $1; }; f"
	pr = "!f() { git fetch origin refs/pull/$1/head:refs/remotes/origin/pr/$1 && git checkout FETCH_HEAD && git remote prune origin; }; f"
	s = status
[core]
	editor = /usr/local/bin/nvim
	pager = diff-so-fancy | less --tabs=4 -RFX
	excludesfile = /Users/brian/.gitignore
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
